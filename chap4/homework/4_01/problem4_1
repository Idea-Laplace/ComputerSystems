0x118: 700c01000000000000   | 	jmp loop	

0x100:                         | .pos 0x100
0x100: 30 f 3 0f00000000000000 | 	irmovq $15, %rbx   // f: no register, 3: %rbx, 0f: $15 immediate
0x10a: 20 3 1                  | 	rrmovq %rbx, %rcx  // 3: %rbx, 1: %rcx
0x10c:                         | loop:				   // loop at 0x10c
0x10c: 40 1 3 fdffffffffffffff | 	rmmovq %rcx, -3(%rbx) // fd: 1111 1101, hence -3 immediate
